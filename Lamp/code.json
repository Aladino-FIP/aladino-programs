[{
        "id": 0,
        "on": {
            "name": "lamp_on",
            "text": "Accendi luce",
            "type": "button",
            "active": true,
            "classes": "btn-success"
        },
        "code": "SendValue(vars.lamp_pin, 255); History('lamp.on')"
    },
    {
        "id": 1,
        "on": {
            "name": "lamp_off",
            "text": "Spegni luce",
            "type": "button",
            "active": false,
            "classes": "btn-danger"
        },
        "code": "SendValue(vars.lamp_pin, 0); History('lamp.off');"
    },
    {
        "id": 2,
        "code": "SetControlActive('lamp_on', false); SetControlActive('lamp_off', true);",
        "onPin": {
            "at": "hi",
            "pin_var": "lamp_pin"
        }
    },
    {
        "id": 3,
        "code": "SetControlActive('lamp_on', true); SetControlActive('lamp_off', false);",
        "onPin": {
            "at": "lo",
            "pin_var": "lamp_pin"
        }
    }
]